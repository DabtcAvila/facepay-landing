{
  "name": "facepay-landing",
  "version": "2.1.0",
  "description": "FacePay - Pagos Crypto con Face ID - Optimizado para Lighthouse 100/100",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "build:analyze": "NODE_ENV=production ANALYZE=true webpack --mode production",
    "build:prerender": "npm run build && npm run prerender",
    "prerender": "node scripts/prerender.js",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:fonts": "node scripts/optimize-fonts.js",
    "serve": "http-server dist -p 8000 --gzip --brotli",
    "lighthouse": "lighthouse http://localhost:8000 --output html --output-path ./lighthouse-report.html",
    "test:performance": "npm run build && npm run serve & sleep 2 && npm run lighthouse && pkill -f http-server",
    "deploy": "npm run build && vercel --prod",
    "clean": "rimraf dist",
    "visual:test": "node tests/visual/visual-testing-orchestrator.js",
    "visual:test-dev": "npm run serve & sleep 3 && npm run visual:test http://localhost:8000 && pkill -f http-server",
    "visual:basic": "node tests/visual/visual-testing-automation.js",
    "visual:cross-browser": "node tests/visual/cross-browser-visual-testing.js",
    "visual:interactive": "node tests/visual/interactive-elements-testing.js",
    "visual:journeys": "node tests/visual/user-journey-simulation.js",
    "visual:regression": "node tests/visual/visual-regression-detection.js",
    "visual:performance": "node tests/visual/performance-visual-correlation.js",
    "visual:baseline": "node tests/visual/visual-regression-detection.js --create-baseline",
    "visual:quick": "npm run visual:basic && npm run visual:interactive",
    "visual:full": "npm run build && npm run visual:test-dev"
  },
  "keywords": ["facepay", "crypto", "starknet", "face-id", "payments", "performance", "lighthouse", "web-vitals"],
  "author": "FacePay Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "style-loader": "^3.3.0",
    "postcss": "^8.4.0",
    "postcss-loader": "^7.3.0",
    "autoprefixer": "^10.4.0",
    "postcss-preset-env": "^8.4.0",
    "cssnano": "^6.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.0",
    "postcss-merge-rules": "^6.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "workbox-webpack-plugin": "^7.0.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "compression-webpack-plugin": "^10.0.0",
    "imagemin-webpack-plugin": "^2.4.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-svgo": "^10.0.0",
    "sharp": "^0.32.0",
    "puppeteer": "^20.6.0",
    "playwright": "^1.40.0",
    "http-server": "^14.1.0",
    "lighthouse": "^10.4.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "core-js": "^3.31.0"
  },
  "browserslist": [
    "> 0.25%",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "performance": {
    "maxAssetSize": 100000,
    "maxEntrypointSize": 100000
  }
}